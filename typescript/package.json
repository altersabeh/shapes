{
  "name": "shapes",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc",
    "archive": "pnpm pack --out dist/tarballs/%s-%v.tgz",
    "fmt": "prettier --write **/*.ts",
    "fmt:check": "prettier --check **/*.ts",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "start": "jiti ./bin/main.ts",
    "test": "vitest --run",
    "test:cov": "vitest --run --coverage",
    "test:watch": "vitest --watch"
  },
  "bin": {
    "shapes-app": "./bin/main.ts"
  },
  "type": "module",
  "main": "lib/index.ts",
  "exports": {
    ".": "./lib/index.ts",
    "./client/shell": "./lib/client/shell/index.ts",
    "./base/models": "./lib/base/models/index.ts",
    "./base/commons": "./lib/base/commons/index.ts"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@stylistic/eslint-plugin-js": "^4.2.0",
    "@types/node": "^22.13.13",
    "@vitest/coverage-v8": "^3.0.9",
    "eslint": "^9.23.0",
    "globals": "^16.0.0",
    "jiti": "^2.4.2",
    "prettier": "^3.5.3",
    "turbo": "^2.4.4",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0",
    "vitest": "^3.0.9"
  },
  "packageManager": "pnpm@10.7.0"
}
